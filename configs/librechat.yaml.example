# LibreChat Configuration Example for n8n + MCP + LM Studio Integration

version: 1.2.9
cache: true

# MCP Servers Configuration
mcpServers:
  n8n:
    type: streamable-http
    url: http://localhost:3000/mcp
    timeout: 60000
    headers:
      Authorization: "Bearer YOUR-SECURE-TOKEN-HERE"

# Custom Endpoints
endpoints:
  custom:
    # LM Studio - Local Models
    - name: 'LM Studio'
      apiKey: 'lm-studio'
      baseURL: 'http://YOUR-LM-STUDIO-IP:1234/v1'  # Change to your LM Studio IP
      models:
        default: ['local-model']
        fetch: true  # Automatically fetches available models
      titleConvo: true
      titleModel: 'local-model'
      modelDisplayLabel: 'LM Studio'
      iconURL: 'https://lmstudio.ai/favicon.ico'
